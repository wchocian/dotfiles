// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "shift+alt+up",
        "command": "-editor.action.insertCursorAbove",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+down",
        "command": "-editor.action.insertCursorBelow",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+down",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+down",
        "command": "-editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+up",
        "command": "editor.action.copyLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+up",
        "command": "-editor.action.copyLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.terminal.focus"
    },
    {
        "key": "ctrl+j",
        "command": "-workbench.action.togglePanel"
    },
    {
        "key": "ctrl+k j",
        "command": "workbench.action.focusActiveEditorGroup"
    },
    {
        "key": "ctrl+shift+`",
        "command": "-workbench.action.terminal.new",
        "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
    },
    {
        "key": "ctrl+tab",
        "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
        "when": "!activeEditorGroupEmpty"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "-workbench.action.quickOpenLeastRecentlyUsedEditorInGroup",
        "when": "!activeEditorGroupEmpty"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.quickOpenNavigatePreviousInViewPicker"
    },
    {
        "key": "ctrl+tab",
        "command": "workbench.action.quickOpenNavigateNextInViewPicker"
    },
    {
        "key": "ctrl+t",
        "command": "-workbench.action.showAllSymbols"
    },
    {
        "key": "ctrl+t",
        "command": "cSpell.goToNextSpellingIssueAndSuggest"
    },
    {
        "key": "ctrl+k ctrl+m",
        "command": "extension.runMake"
    },
    {
        "key": "ctrl+k \\",
        "command": "editor.action.selectToBracket"
    },
    {
        "key": "ctrl+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+b",
        "command": "editor.action.insertSnippet",
        "when": "editorHasSelection && editorLangId == latex",
        "args": {
            "snippet": "\\textbf{${TM_SELECTED_TEXT}}"
        }
    },
    {
        "key": "ctrl+i",
        "command": "editor.action.insertSnippet",
        "when": "editorHasSelection && editorLangId == latex",
        "args": {
            "snippet": "\\textit{${TM_SELECTED_TEXT}}"
        }
    },
    {
        "key": "ctrl+'",
        "command": "editor.action.insertSnippet",
        "when": "editorHasSelection && editorLangId == latex",
        "args": {
            "snippet": "\\quote{${TM_SELECTED_TEXT}}"
        }
    },
    {
        "key": "ctrl+k ctrl+i",
        "command": "editor.action.insertSnippet",
        "when": "editorHasSelection && editorLangId == latex",
        "args": {
            "snippet": "\\iln{${TM_SELECTED_TEXT}}"
        }
    },
    {
        "key": "ctrl+k shift+-",
        "command": "editor.action.insertSnippet",
        "when": "editorHasSelection && editorLangId == latex",
        "args": {
            "snippet": "\\tsub{${TM_SELECTED_TEXT}}"
        }
    },
    {
        "key": "ctrl+k ctrl+l",
        "command": "editor.action.insertSnippet",
        "when": "editorHasSelection && editorLangId == latex",
        "args": {
            "snippet": "<@\\textcolor{blue}{${TM_SELECTED_TEXT}}@>"
        }
    },
    {
        "key": "ctrl+alt+b",
        "when": "editorLangId == python",
        "command": "python.execInTerminal-icon"
    },
    {
        "key": "ctrl+shift+f",
        "command": "-workbench.view.search",
        "when": "workbench.view.search.active && neverMatch =~ /doesNotMatch/"
    },
    {
        "key": "ctrl+shift+f",
        "command": "-workbench.action.findInFiles"
    },
    {
        "key": "ctrl+alt+b",
        "when": "editorLangId == asciidoc",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
        "text": "asciidoctor-pdf --theme ~/GIT/pze-swis-5/RESOURCES/THEMES/basic-theme.yml ${file}\r"
        }
    },
    {
        "key": "ctrl+i",
        "when": "editorHasSelection && editorLangId == asciidoc",
        "command": "editor.action.insertSnippet",
        "args": {
            "snippet": "_${TM_SELECTED_TEXT}_"
        }
    },
    {
        "key": "ctrl+b",
        "when": "editorHasSelection && editorLangId == asciidoc",
        "command": "editor.action.insertSnippet",
        "args": {
            "snippet": "*${TM_SELECTED_TEXT}*"
        }
    },
    {
        "key": "ctrl+'",
        "when": "editorHasSelection && editorLangId == asciidoc",
        "command": "editor.action.insertSnippet",
        "args": {
            "snippet": "&#8222;${TM_SELECTED_TEXT}&#8221;"
        }
    },
    {
        "key": "shift+-",
        "when": "editorHasSelection && editorLangId == latex",
        "command": "editor.action.insertSnippet",
        "args": {
            "snippet": "_{${TM_SELECTED_TEXT}}"
        }
    },
    {
        "key": "shift+6",
        "when": "editorHasSelection && editorLangId == latex",
        "command": "editor.action.insertSnippet",
        "args": {
            "snippet": "^{${TM_SELECTED_TEXT}}"
        }
    },
    {
        "key": "ctrl+k c",
        "when": "editorHasSelection && editorLangId == latex",
        "command": "editor.action.insertSnippet",
        "args": {
            "snippet": "\\textcolor{${1:color}}{${TM_SELECTED_TEXT}}$0"
        }
    },
    {
        "key": "alt+c",
        "when": "editorHasSelection && editorLangId == latex",
        "command": "editor.action.insertSnippet",
        "args": {
            "snippet": "\\\\${1:command}{${TM_SELECTED_TEXT}}$0"
        }
    },
    {
        "key": "shift+4",
        "when": "editorHasSelection && editorLangId == markdown",
        "command": "editor.action.insertSnippet",
        "args": {
            "snippet": "$${TM_SELECTED_TEXT}$"
        }
    },
    {
        "key": "ctrl+k c",
        "command": "-workbench.files.action.compareWithClipboard"
    },
    {
        "key": "ctrl+k d",
        "command": "-workbench.files.action.compareWithSaved"
    },
    {
        "key": "ctrl+k ctrl+alt+b",
        "command": "makefile.buildTarget"
    },
    {
        "key": "ctrl+k a",
        "command": "spellright.addFromSelectionToUserDictionary"
    },
    {
        "key": "alt+c",
        "command": "-toggleSearchEditorCaseSensitive",
        "when": "inSearchEditor && searchInputBoxFocus"
    },
]

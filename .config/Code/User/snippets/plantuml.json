{
	// Place your snippets for plantuml here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Simple template": {
		"prefix": "puml_template",
		"body": [
			"@startuml ${1:Test} diagram",
			"skinparam {",
			"    RoundCorner 20",
			"    Linetype Ortho",
			"}",
			"",
			"skinparam Class {",
			"    AttributeIconSize 0",
			"    HeaderBackgroundColor LightYellow",
			"    BorderColor Black",
			"    BorderThickness 1.5",
			"    FontName Ubuntu",
			"    FontName TeX Gyre Adventor",
			"    FontSize 16",
			"}",
			"",
			"skinparam Arrow {",
			"    Color Black",
			"    FontName Ubuntu",
			"    FontName TeX Gyre Adventor",
			"    FontSize 10",
			"    Thickness 1.5",
			"}",
			"",
			"class $0Main {",
			"    ~{static} kwadrat() : double",
			"    +static main(String[] args) : void",
			"}",
			"",
			"@enduml"
		],
		"description": "Prepare simple template"
	}
}